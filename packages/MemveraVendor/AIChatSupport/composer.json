{
    "name": "memveravendor/aichatsupport",
    "description": "A Laravel package to integrate ChatGPT and Pinecone for AI-powered support.",
    "type": "library",
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^9.0 || ^11.0",
        "phpoffice/phpword": "^0.18.2 || ^1.2",
        "smalot/pdfparser": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "MemveraVendor\\AIChatSupport\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MemveraVendor\\AIChatSupport\\AIChatSupportServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
